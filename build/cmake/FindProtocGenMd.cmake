IF (WIN32)
	SET(EXE_NAME protoc-gen-md.cmd)
ELSE(WIN32)
	SET(EXE_NAME protoc-gen-md)
ENDIF(WIN32)
FIND_PROGRAM(PROTOC_GEN_MD_BIN
	NAMES
		${EXE_NAME}
	PATHS 
		${CMAKE_SOURCE_DIR}/ext/md-protobuf
		${PROTOC_GEN_MD}
		/usr/local/bin/
		/usr/bin/
		${PYTHON_ROOT}/Scripts
)
IF(PROTOC_GEN_MD_BIN)
